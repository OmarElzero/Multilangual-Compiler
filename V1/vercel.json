{
  "version": 2,
  "builds": [
    {
      "src": "web/backend/modern_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web/backend/modern_server.py"
    }
  ],
  "functions": {
    "web/backend/modern_server.py": {
      "runtime": "python3.9"
    }
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9",
      "NODE_VERSION": "18"
    }
  }
}
