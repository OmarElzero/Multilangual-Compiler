{
  "timeout_seconds": 10,
  "log_level": "INFO",
  "supported_languages": ["python", "cpp"],
  "log_file": "logs/output.log",
  "error_handling": {
    "capture_errors": true,
    "log_errors": true,
    "error_format": "json"
  },
  "docker_image": "multilang_compiler:latest",
  "docker_network": "multilang_network",
  "docker_memory_limit": "512m",
  "docker_cpu_quota": 50000,
  "docker_disk_quota": "1g",
  "docker_security_profile": "default",
  "docker_cleanup": true,
  "docker_isolation": true,
  "docker_resource_limits": {
    "cpu": "0.5",
    "memory": "512m",
    "disk": "1g"
  },
  "docker_security_policies": {
    "allow_privileged": false,
    "allow_network": false,
    "allow_mounts": false,
    "allow_capabilities": false,
    "allow_syscalls": false
  },
  "docker_image_optimization": true,
  "docker_container_cleanup": true,
  "docker_execution_timeout": 30,
  "docker_execution_limits": {
    "cpu": "1",
    "memory": "1g",
    "disk": "2g"
  },
  "docker_plugin_architecture": true,
  "docker_caching_system": true,
  "docker_dependency_management": true,
  "web_interface": {
    "frontend_framework": "React",
    "editor": "Monaco Editor",
    "backend_framework": "FastAPI",
    "api_endpoint": "/run",
    "syntax_highlighting": true,
    "progress_indicators": true,
    "real_time_output": true,
    "interactive_mode": true
  },
  "save_share_projects": {
    "database": "PostgreSQL",
    "sharable_urls": true,
    "project_saving": true,
    "collaboration_features": {
      "real_time_editing": true,
      "comments": true,
      "community_support": true
    }
  },
  "full_compiler_features": {
    "optimizer": true,
    "linker": true,
    "build_system": true
}
}
